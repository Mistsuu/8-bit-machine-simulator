ldi 105
sta n           # n = 105

rebegin:
ldi 1
sta d           # d = 1

restart:
lda n
sub d           # _areg = n - d
jc start
jmp rebegin

start:
lda n           # _areg = n

loop:
sub d           # _areg = _areg - d
jz output       # if n - k*d == 0, then d divides n
jc loop
jmp increment

output:
lda d
out

increment:
lda d
aei 1
sta d
jmp restart
